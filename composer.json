{
    "name": "eviweb/laravel4-workbench",
    "description": "Extended Laravel4 workbench command",
    "keywords": ["extension", "laravel", "workbench", "command"],
    "license" : "MIT",
    "authors": [
        {
            "name": "Eric VILLARD",
            "email": "dev@eviweb.fr",
            "homepage": "https://github.com/eviweb",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0 <7.0",
        "illuminate/support": "4.x",
        "illuminate/workbench": "4.x",
        "eviweb/illuminate-support": "~0.1"
    },
    "require-dev": {
        "mikey179/vfsstream": "~1.6",
        "phpunit/phpunit": "3.7.*@dev",
        "mockery/mockery": "~0.9",
        "antecedent/patchwork": "*",
        "illuminate/config": "4.x",
        "illuminate/events": "4.x",
        "illuminate/console": "4.x"
    },
    "autoload": {
        "psr-0": {
            "evidev": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}